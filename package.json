{
  "name": "timeTracker",
  "productName": "timeTracker",
  "version": "2.5.1",
  "description": "timeTracker, app for help planning your day",
  "scripts": {
    "dev": "cross-env-shell BUILD_STAGE=local \"run-p watch:render watch:electron\"",
    "unitTests:watch": "jest -c ./jest.json --verbose --watchAll --coverage",
    "dist:AppImage": "cross-env-shell TARGET_PACKAGE=AppImage UPDATE_FOLDER=v4 yarn dist",
    "dist:deb": "cross-env-shell TARGET_PACKAGE=deb yarn dist",
    "dist:snap": "cross-env-shell TARGET_PACKAGE=snap yarn dist",
    "dist:brew": "cross-env-shell TARGET_PACKAGE=brew UPDATE_FOLDER=v4 yarn dist",
    "dist:dmg": "cross-env-shell TARGET_PACKAGE=dmg yarn dist",
    "dist:nsis": "cross-env-shell TARGET_PACKAGE=nsis yarn dist",
    "dist:nsis:ia32-x64": "cross-env-shell TARGET_PACKAGE=nsis TARGET_ARCH=ia32 yarn dist",
    "dist": "cross-env-shell NODE_ENV=production run-s build pack",
    "unitTests": "jest -c ./jest.json --verbose --coverage",
    "pack": "node ./scripts/electronBuild.js",
    "build": "run-p build:render build:electron",
    "build:render": "webpack --config webpack.config.render.js",
    "build:electron": "webpack --config webpack.config.electron.js",
    "compress": "node utils/compress.js",
    "test": "yarn unitTests",
    "testts": "tsc --pretty --outDir dist -p tsconfig.json",
    "prettyjs": "pretty-quick",
    "prettyjs-with-bail": "pretty-quick --bail",
    "watch:render": "node webpack.server.render.js",
    "watch:electron": "gulp scripts",
    "postinstall": "electron-builder install-app-deps",
    "version-patch": "npm version patch -m \"Bump version to %s\"",
    "version-minor": "npm version minor -m \"Bump minor version to %s\"",
    "postversion": "auto-changelog -p && git add -A && git commit -am \"Update Changelog\" && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.qarea.org/tgms/tgExtension"
  },
  "build-version": "BUILD",
  "author": {
    "name": "test Inc.",
    "email": "timeTracker@dasdasd.com",
    "url": "https://diligences.com/"
  },
  "homepage": "https://diligences.com/",
  "license": "ISC",
  "main": "./build-electron-main/main.js",
