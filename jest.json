{
  "rootDir": ".",
  "testRegex": "\\.test\\.(tsx|ts)?$",
  "moduleFileExtensions": ["js", "jsx", "json", "ts", "tsx"],
  "setupFiles": ["<rootDir>/test-helpers/polyfills.js", "<rootDir>/test-helpers/setupTest.js"],
  "testURL": "http://localhost/",
  "moduleNameMapper": {
    "lodash-es/(.*)": "lodash/$1",
    "@api": "<rootDir>/test-helpers/api.mock.ts",
    "redux-saga/effects": "<rootDir>/test-helpers/effects.mock.ts",
    "react-redux": "<rootDir>/test-helpers/react-redux.mock.ts",
    "auto-launch": "<rootDir>/test-helpers/autoLaunch.mock.ts",
    "material-icons-react": "<rootDir>/test-helpers/icons.mock.tsx",
    ".*(\\.svg|\\.png)": "<rootDir>/test-helpers/image-resolver.ts",
    "@images/images": "<rootDir>/test-helpers/image-resolver.ts",
    ".*(\\.scss|\\.css)": "identity-obj-proxy",
    "@electronMain/*": "<rootDir>/test-helpers/electronMain.mock.ts",
    "@test-helpers/(.*)": "<rootDir>/test-helpers/$1.ts",
    "@services/time": "<rootDir>/test-helpers/time.mock.ts",
    "@store/plannings/actions": "<rootDir>/store/plannings/actions.ts",
    "@store/auth/utils": "<rootDir>/test-helpers/auth.ts",
    "@services/useShallowEqualSelector": "<rootDir>/services/useShallowEqualSelector.ts",
    "@services/formater": "<rootDir>/services/formater.ts",
    "@services/helpers": "<rootDir>/services/helpers.ts",
    "@services/analytics": "<rootDir>/services/analytics.ts",
    "@services/call-api": "<rootDir>/services/call-api.ts",
    "@store/auth/actions": "<rootDir>/store/auth/actions.ts",
    "@store/internetConnection/watcher": "<rootDir>/store/internetConnection/watcher.ts",
    "@store/notifications/actions": "<rootDir>/store/notifications/actions.ts",
    "@store/currentUser/actions": "<rootDir>/store/currentUser/actions.ts",
    "@components/Notification": "<rootDir>render/src/Components/Notification",
    "@components/PasswordHint": "<rootDir>render/src/Components/PasswordHint",
    "@utils": "<rootDir>utils",
    "@services/api-types": "<rootDir>services/api-types",
    "@components/IconWrapper": "<rootDir>render/src/Components/IconWrapper",
    "@components/SearchBlock": "<rootDir>render/src/Components/SearchBlock",
    "@components/PlanningModal": "<rootDir>render/src/Components/PlanningModal",
    "@components/TimeSelector": "<rootDir>render/src/Components/TimeSelector",
    "@components/Issue": "<rootDir>render/src/Components/Issue",
    "@components/Project": "<rootDir>render/src/Components/Project",
    "@components/Spinner": "<rootDir>render/src/Components/Spinner",
    "@store/alerts/actions": "<rootDir>/store/alerts/actions.ts",
    "@services/microservices/avatar": "<rootDir>services/microservices/avatar.ts",
    "@services/extendIssues": "<rootDir>/services/extendIssues",
    "@services/api": "<rootDir>services/api.ts",
    "@store/bookmarks/actions": "<rootDir>/store/bookmarks/actions.ts",
    "@services/openWindow": "<rootDir>/services/openWindow.ts",
    "@store/history": "<rootDir>/store/history/index.ts",
    "@services/sort": "<rootDir>/services/sort.ts",
    "@store/newDay/actions": "<rootDir>/store/newDay/actions.ts",
    "@services/typesValidation": "<rootDir>/services/typesValidation.ts",
    "@store/projects/actions": "<rootDir>/store/projects/actions"
  },
  "transform": {
    ".(t|j)sx?": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "restoreMocks": true
}
